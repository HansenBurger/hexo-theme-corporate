<div class="row">
  <% if(index==true){ %>
  <div class="col-xs-12 blog-posts">
    <% } else { %>
    <div class="col-md-9 col-sm-9 blog-posts">
      <% } %> <% if (pagination==2){ %> <% page.posts.each(function(post){ %>
      <%- partial('article', {post: post, index: true}) %> <% }) %> <% if
      (page.total> 1){ %>
      <nav id="page-nav">
        <%- paginator({ prev_text: '&laquo; Prev' , next_text: 'Next &raquo;' })
        %>
      </nav>
      <% } %> <% } else { %>
      <section class="archives-wrap">
        <div class="archives">
          <% if(showLatest==false) { %> 
            <% page.posts.each(function(post, i){ %>
              <%- partial('archive-post', {post: post, even: i % 2==0, index: index}) %> 
            <% }) %> 
          <% } else { %> 
            <% if(typeof showSelected !== 'undefined') { %>
                <% page.posts.each(function (post, i) { %>
                  <% if(post.id.includes('A')) { %>
                    <!-- debug-test -->
                    <!--<div>post.id = <%- post.id %></div>-->
                    <%- partial('archive-post', {post: post, even: i % 2==0, index: index}) %>
                  <% } %>
                <% }) %>
            <% } %>
          <% } %>
        </div>
      </section>
      <% if (showLatest==true) {%>
      <!--<h4 style="padding-left: 10px; padding-bottom: 15px">
        <a href="/archives">See All</a>
      </h4>-->
      <% } %> <% } %>
    </div>
    <% if(index==false){ %>
    <div class="col-md-3 col-sm-3 blog-sidebar"><%- partial('sidebar') %></div>
    <% } %>
  </div>
</div>
